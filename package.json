  "repository": {
    "type": "git",
    "url": "git+https://github.com/CuteNikki/discord-bot.git"
  },
  "module": "./src/index.ts",
  "dependencies": {
    "@translate-tools/core": "^2.1.0",
    "@types/react": "19.2.5",
    "canvacord": "6.0.4",
    "cron": "^4.3.3",
    "dayjs": "1.11.19",
    "discord-actions": "^2.3.13",
    "discord-html-transcripts": "3.3.0",
    "discord-hybrid-sharding": "3.0.1",
    "discord.js": "14.24.2",
    "i18next": "^25.3.4",
    "i18next-fs-backend": "2.6.1",
    "mongoose": "8.19.4",
    "ms": "^2.1.3",
    "node-os-utils": "2.0.1",
    "pino": "10.1.0",
    "pino-pretty": "^13.1.1"
  },
  "devDependencies": {
    "@eslint/compat": "2.0.0",
    "@types/bun": "1.3.2",
    "@types/ms": "^2.1.0",
    "@types/node-os-utils": "^1.3.4",
    "@typescript-eslint/eslint-plugin": "8.46.4",
    "@typescript-eslint/parser": "8.46.4",
    "eslint": "9.39.1",
    "prettier": "^3.6.2",
    "typescript": "5.9.3"
  },
  "bugs": {
    "url": "https://github.com/CuteNikki/discord-bot/issues"
  },
  "description": "DiscordJS Bot written in TypeScript",
  "homepage": "https://github.com/CuteNikki/discord-bot",
  "license": "MIT",
  "scripts": {
    "dev": "bun run ./src/index.ts",
    "deploy": "bun run ./src/structure/utilities/register.ts",
    "start": "bun run ./dist/src/index.js",
    "build": "rm -rf dist && bun tsc --build ./",
    "pretty": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\"",
    "lint": "eslint --ignore-pattern \"locales/*\"",
    "dellog": "rm pino.log"
  },
  "prettier": {
    "printWidth": 160,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "trailingComma": "none"
  },
  "type": "module"
}
